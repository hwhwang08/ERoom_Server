{
  "version": 2,
  "functions": {
    "api/server.js": {
      "runtime": "nodejs18.0.0"
    }
  },
  "builds": [
  {
    "src": "api/server.js",
    "use": "@vercel/node"
  }
],
  "installCommand": "npm install --platform=linux --arch=x64",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/firebase-config",
      "dest": "/api/server.js"
    },
    {
      "src": "/verify-token",
      "dest": "/api/server.js"
    },
    {
      "src": "/verify-user-and-payment",
      "dest": "/api/server.js"
    },
    {
      "src": "/purchase",
      "dest": "/api/server.js"
    },
    {
      "src": "/payment-complete",
      "dest": "/api/server.js"
    },
    {
      "src": "/save-uid",
      "dest": "/api/server.js"
    },
    {
      "src": "/success",
      "dest": "/api/server.js"
    },
    {
      "src": "/iamport-webhook",
      "dest": "/api/server.js"
    },
    {
      "src": "/login",
      "dest": "/api/server.js"
    },
    {
      "src": "/",
      "dest": "/api/server.js"
    },
    {
      "src": "/health",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)\\.(html)$",
      "dest": "/public/$1.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.js"
    }
  ]
}