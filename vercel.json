{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/firebase-config",
      "dest": "/api/index.js"
    },
    {
      "src": "/verify-token",
      "dest": "/api/index.js"
    },
    {
      "src": "/verify-user-and-payment",
      "dest": "/api/index.js"
    },
    {
      "src": "/purchase",
      "dest": "/api/index.js"
    },
    {
      "src": "/payment-complete",
      "dest": "/api/index.js"
    },
    {
      "src": "/save-uid",
      "dest": "/api/index.js"
    },
    {
      "src": "/success",
      "dest": "/api/index.js"
    },
    {
      "src": "/iamport-webhook",
      "dest": "/api/index.js"
    },
    {
      "src": "/login",
      "dest": "/api/index.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
}