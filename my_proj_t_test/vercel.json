{
  "functions": {
    "api/*.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index"
    },
    {
      "source": "/login",
      "destination": "/public/login.html"
    },
    {
      "source": "/firebase-config.js",
      "destination": "/api/firebase-config"
    }
  ]
}